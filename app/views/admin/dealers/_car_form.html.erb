<%= form_for(car, url: {controller: "dealers", action: "create_car"}) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :count %>
    <%= f.text_field :count, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :car_image %>
    <%= f.file_field :car_image, id: :car_image%>
  </div>

  <%= f.submit "登録する", class: "btn btn-primary" %>
<% end %>
