<%= form_with model: [:admin, stock], local: true do |f| %>
  <div class="form-group">
    <%= f.label :date %>
    <%= f.text_field :date, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :time %>
    <%= f.text_field :time, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :availables %>
    <%= f.text_field :availables, class: "form-control" %>
  </div>

  <%= f.submit "登録する", class: "btn btn-primary" %>
<% end %>
